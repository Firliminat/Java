import java.util.Arrays;

public class HelloWorld {
	public static void main(String args[]) {
		double Variable = 0;
		
		Variable += 1;
		System.out.print(Variable);
		
		String[] mots = new String[] {"hello", "world", "!"};
		Arrays.stream(mots).forEach(mot -> {
			System.out.print(mot+" ");
		});
	}
}

