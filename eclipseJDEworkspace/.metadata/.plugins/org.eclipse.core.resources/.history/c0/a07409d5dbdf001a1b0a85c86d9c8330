import java.util.Arrays;

public class HelloWorld {
	public static void main(String args[]) {
		int n_carré = 0;
		
		n_carré += 1;
		System.out.print(n_carré);
		
		String[] mots = new String[] {"hello", "world", "!"};
		Arrays.stream(mots).forEach(mot -> {
			System.out.print(mot+" ");
		});
	}
}

